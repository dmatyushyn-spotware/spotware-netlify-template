{
  "name": "@spotware-web-team/sdk",
  "version": "0.0.5",
  "author": "Spotware",
  "license": "ISC",
  "packageManager": "yarn@4.9.1",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "tsc --noEmit",
    "build": "yarn clean && tsup",
    "clean": "rimraf dist",
    "generate": "ts-node generator/generate.ts",
    "push": "yarn build && yarn version patch && yarn npm publish"
  },
  "devDependencies": {
    "@spotware-web-team/sdk-external-api": "0.0.1",
    "@types/fs-extra": "11.0.4",
    "@types/mustache": "4.2.5",
    "@types/node": "22.13.10",
    "@veksa/protobuf-parser": "1.0.1",
    "fs-extra": "11.3.0",
    "mustache": "4.2.0",
    "rimraf": "^6.0.1",
    "rxjs": "7.8.2",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "tsup": "^8.4.0",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "uuid": "^11.1.0"
  }
}
